{
  "name": "rollup-plugin-transpile",
  "version": "0.2.2",
  "description": "A Rollup plugin to transpile Ecmascript and Typescript",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "author": "Nicola Krumschmidt",
  "license": "MIT",
  "keywords": [
    "rollup",
    "plugin",
    "transpile",
    "transform",
    "ecmascript",
    "typescript"
  ],
  "peerDependencies": {
    "rollup": "^3.0.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@swc/core": "^1.3.36"
  },
  "devDependencies": {
    "@types/node": "^16.7.4",
    "prettier": "^2.8.4",
    "rollup": "^3.17.2",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-transpile": "^0.2.2",
    "typescript": "^4.9.5"
  }
}
